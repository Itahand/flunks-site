# Add these to your .env.local file for XYZ wallet integration

# Dynamic Labs API Token (get from https://app.dynamic.xyz/dashboard/developer/api)
DYNAMIC_API_TOKEN=dyn_your_api_token_here

# Your XYZ Wallet API Configuration
XYZ_WALLET_API_ENDPOINT=https://api.yourxyzwallet.com
XYZ_WALLET_API_KEY=your_xyz_wallet_api_key_here

# Optional: XYZ Wallet specific settings
NEXT_PUBLIC_XYZ_WALLET_API=https://api.yourxyzwallet.com
XYZ_WALLET_NETWORK=mainnet
XYZ_WALLET_VERSION=v1
